{
  "name": "imikemiller/l5-swagger-redoc",
  "description": "Swagger integration to Laravel 5 using the Rebilly Redoc UI.",
  "keywords": [
    "laravel",
    "swagger",
    "api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Darius Matulionis",
      "email": "darius@matulionis.lt"
    },
    {
      "name": "Mike Miller",
      "email": "iam@mikemiller.uk"
    }
  ],
  "require": {
    "php": "^7.0",
    "laravel/framework": "^5.4",
    "zircote/swagger-php": "~2.0",
    "swagger-api/swagger-ui": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "6.*",
    "mockery/mockery": "0.9.*",
    "orchestra/testbench": "~3.4",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "L5Swagger\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": false
}
